a = 5
b = 'string'
c = 10
d = a * c

print (a)
print (d, b)

string = input("please input string: ")
print(string)

string2 = input("please input one more string: ")

number = input("please input number: ")
number = int(number)
print (number)

number2 = input("please input one more number: ")
number2 = int(number2)
print (number2)

print (string, type(string))
print(number, type(number))
print (number2, type(number2))

print(number + number2)
print(string + string2)
